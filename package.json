  "repository": {
    "type": "git",
    "url": "https://github.com/ajlende/ajlende.github.io.git"
  },
  "dependencies": {
    "autoprefixer": "^7.1.3",
    "bloomer": "^0.6.2",
    "bulma": "^0.5.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "gatsby": "^1.9.17",
    "gatsby-link": "^1.6.15",
    "gatsby-plugin-catch-links": "^1.0.8",
    "gatsby-plugin-google-analytics": "^1.0.7",
    "gatsby-plugin-postcss-sass": "^1.0.10",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-sass": "^1.0.10",
    "gatsby-remark-copy-linked-files": "^1.5.7",
    "gatsby-remark-images": "^1.5.10",
    "gatsby-remark-prismjs": "^1.2.7",
    "gatsby-remark-responsive-iframe": "^1.4.7",
    "gatsby-remark-smartypants": "^1.4.7",
    "gatsby-source-filesystem": "^1.4.12",
    "gatsby-transformer-remark": "^1.7.6"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "prettier": "^1.6.1"
  },
  "main": "public/index.html",
  "scripts": {
    "postinstall": "find node_modules/bulma -name '*.sass' | xargs sed -Ei '' 's/^\\.(tag|number)$/.\\1:not(.token)/g'",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public --branch master",
    "format": "prettier --print-width 100 --trailing-comma all --no-semi --write src/**/*.{js,jsx}",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
